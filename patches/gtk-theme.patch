diff -rupN gtk-master-orig/gtk/gtkrc gtk-master/gtk/gtkrc
--- gtk-master-orig/gtk/gtkrc	1970-01-01 02:00:00.000000000 +0200
+++ gtk-master/gtk/gtkrc	2014-06-14 14:28:50.847990242 +0300
@@ -0,0 +1,52 @@
+style "step" {
+	bg[ACTIVE] = "#9c9c9c"
+	fg[ACTIVE] = "#000000"
+	base[ACTIVE] = "#b5b5b5"
+
+	bg[NORMAL] = "#b5b5b5"
+	fg[NORMAL] = "#000000"
+	base[NORMAL] = "#e8e8e8"
+
+	bg[INSENSITIVE] = "#b5b5b5"
+	fg[INSENSITIVE] = "#777777"
+	base[INSENSITIVE] = "#9c9c9c"
+
+	bg[PRELIGHT] = "#778394"
+	fg[PRELIGHT] = "#e8e8e8"
+	base[PRELIGHT] = "#b5b5b5"
+
+	bg[SELECTED] = "#778394"
+	fg[SELECTED] = "#e8e8e8"
+	base[SELECTED] = "#486591"
+
+	font = "-*-lucida-medium-r-*-*-10-*-*-*-*-*-*-*"
+}
+
+style "menu" {
+	bg[INSENSITIVE] = "#b5b5b5"
+}
+
+style "whitebg" {
+	base[NORMAL] = "#e8e8e8"
+}
+
+style "button_bold" {
+	font = "-*-lucida-bold-r-*-*-10-*-*-*-*-*-*-*"
+}
+
+style "hilight" {
+	fg[PRELIGHT] = "#ffffff"
+	fg[ACTIVE] = "#ffffff"
+}
+
+class "GtkWidget" style "step"
+widget "*GtkButton*" style "button_bold"
+
+widget "*Menu*" style "hilight"
+widget "*Option*" style "hilight"
+widget "*Item*" style "hilight"
+widget "*Accel*" style "hilight"
+
+class "GtkMenu" style "menu"
+class "GtkCList" style "whitebg"
+class "GtkEntry" style "whitebg"
